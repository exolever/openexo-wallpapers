#! /bin/sh
set -e

case "$1" in
    configure)
        # Workaround LP: #746028
        touch /usr/share/backgrounds/wallpaper-exo-1.jpg
    ;;

    abort-upgrade|abort-deconfigure|abort-remove)
    ;;

    *)
        echo "$0 called with unknown argument \`$1'" 1>&2
        exit 1
    ;;
esac

